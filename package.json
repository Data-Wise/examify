{
  "name": "examify",
  "version": "0.4.2",
  "description": "Create exams from Markdown and export to Canvas QTI format",
  "main": "dist/index.js",
  "bin": {
    "examify": "dist/index.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Data-Wise/examify.git"
  },
  "homepage": "https://data-wise.github.io/examify/",
  "bugs": {
    "url": "https://github.com/Data-Wise/examify/issues"
  },
  "author": "Data-Wise",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "canvas",
    "qti",
    "quiz",
    "exam",
    "lms",
    "education",
    "converter",
    "markdown",
    "quarto"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "version": "bun scripts/sync-version.ts && git add -A",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "fast-xml-parser": "^5.3.2"
  }
}
