{
  "name": "canvas-qti-converter",
  "version": "0.1.0",
  "description": "Convert Markdown/Text question files to Canvas-compatible QTI format",
  "main": "dist/index.js",
  "bin": {
    "qti-convert": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js",
    "test": "vitest"
  },
  "keywords": ["canvas", "qti", "quiz", "lms", "education", "converter"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "fast-xml-parser": "^5.3.2"
  }
}
