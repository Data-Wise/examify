    <section ident="mediation_analysis" title="Mediation Analysis">
      <rubric><material><mattext>Answer based on the Baron &amp; Kenny framework.</mattext></material></rubric>
      <item ident="q4" title="Question 4">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>2</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">In mediation, the indirect effect equals:</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="a">
                <material>
                  <mattext texttype="text/html">c (the total effect)</mattext>
                </material>
              </response_label>
              <response_label ident="b">
                <material>
                  <mattext texttype="text/html">a Ã— b (the product of paths a and b)</mattext>
                </material>
              </response_label>
              <response_label ident="c">
                <material>
                  <mattext texttype="text/html">c&apos; (the direct effect)</mattext>
                </material>
              </response_label>
              <response_label ident="d">
                <material>
                  <mattext texttype="text/html">a + b</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="Decimal" defaultval="0" maxvalue="2"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">b</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">2</setvar>
          </respcondition>
        </resprocessing>
      </item>
