  <assessment ident="statistics_final_exam" title="Statistics Final Exam">
    <section ident="descriptive_statistics" title="Descriptive Statistics">
      <rubric><material><mattext>Select the best answer for each question.</mattext></material></rubric>
      <item ident="q1" title="Question 1">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>2</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">The mean of a normal distribution is denoted by \( \mu \). Which statement is true?</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="a">
                <material>
                  <mattext texttype="text/html">It equals both the median and the mode</mattext>
                </material>
              </response_label>
              <response_label ident="b">
                <material>
                  <mattext texttype="text/html">It equals only the median</mattext>
                </material>
              </response_label>
              <response_label ident="c">
                <material>
                  <mattext texttype="text/html">It equals only the mode</mattext>
                </material>
              </response_label>
              <response_label ident="d">
                <material>
                  <mattext texttype="text/html">It is always greater than the median</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar varname="SCORE" vartype="Decimal" defaultval="0" maxvalue="2"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">a</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">2</setvar>
          </respcondition>
        </resprocessing>
      </item>
