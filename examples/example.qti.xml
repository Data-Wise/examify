              <response_label ident="b">
                <material>
                  <mattext texttype="text/html">The value is one standard deviation above the mean</mattext>
                </material>
              </response_label>
              <response_label ident="c">
                <material>
                  <mattext texttype="text/html">The distribution is perfectly normal</mattext>
                </material>
              </response_label>
              <response_label ident="d">
                <material>
                  <mattext texttype="text/html">The variance is zero</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
